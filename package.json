{
  "name": "my-workspace",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dotenv-pull:all": "npm run dotenv-pull --workspaces --if-present --include-workspace-root",
    "dotenv-push:all": "npm run dotenv-push --workspaces --if-present --include-workspace-root",
    "dotenv-pull": "npx dotenv-vault@latest pull",
    "dotenv-push": "npx dotenv-vault@latest push",
    "dotenv:build": "npx dotenv-vault@latest build",
    "build-client": "docker build -t talk2resume/client -f dockerfile.client .",
    "build-api": "docker build -t talk2resume/api -f dockerfile.api .",
    "build-share": "docker build -t talk2resume/share -f packages/file-sharing/Dockerfile ./packages/file-sharing",
    "upgrade-container-prod": "npx tsx ./scripts/deploy-prod.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.4",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@auth0/auth0-angular": "^2.2.3",
    "@dotenvx/dotenvx": "^1.33.0",
    "@types/busboy": "^1.5.4",
    "@types/fs-extra": "^11.0.4",
    "@types/mongodb": "^4.0.6",
    "@types/shelljs": "^0.8.15",
    "ag-grid-angular": "^33.0.3",
    "axios": "^1.7.9",
    "busboy": "^1.6.0",
    "chart.js": "^4.4.7",
    "chromadb": "^1.10.3",
    "chromadb-default-embed": "^2.13.2",
    "cors": "^2.8.5",
    "dexie": "^4.0.10",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "loglevel": "^1.9.2",
    "lowdb": "^7.0.1",
    "memory-streams": "^0.1.3",
    "mongodb": "^6.12.0",
    "node-html-parser": "^7.0.1",
    "ollama": "^0.5.12",
    "punycode": "^2.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "~7.8.0",
    "shelljs": "^0.8.5",
    "three": "^0.172.0",
    "tslib": "^2.3.0",
    "tsx": "^4.19.2",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.13",
    "autoprefixer": "^10.4.20",
