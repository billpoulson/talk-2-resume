{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "@talk2resume/types": {
      "projectType": "library",
      "root": "packages/types",
      "sourceRoot": "packages/types/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/types/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/types/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/types/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "packages/types/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "@talk2resume/client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "packages/client",
      "sourceRoot": "packages/client/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/talk2resume/client",
            "index": "packages/client/src/index.html",
            "browser": "packages/client/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "packages/client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "packages/client/public"
              }
            ],
            "styles": [
              "packages/client/src/styles/index.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kB",
                  "maximumError": "15kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
          },
          "configurations": {
            "production": {
              "buildTarget": "@talk2resume/client:build:production",
              "proxyConfig": "packages/client/proxy.conf.prod.json"
            },
            "development": {
              "buildTarget": "@talk2resume/client:build:development",
              "proxyConfig": "packages/client/proxy.conf.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "packages/client/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "packages/client/public"
              }
            ],
            "styles": [
              "packages/client/src/styles/index.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "@talk2resume/common": {
      "projectType": "library",
      "root": "packages/common",
      "sourceRoot": "packages/common/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/common/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/common/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/common/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "packages/common/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "api": {
      "projectType": "library",
      "root": "packages/api",
      "sourceRoot": "packages/api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "packages/api/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
