{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug API",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "watch:debug",
        "-w=@talk2resume/api"
      ],
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**",
        "**/node_modules/**",
        "**/dist/**"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "preLaunchTask": "watch common"
    },
    {
      "name": "Launch Chrome against localhost",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "userDataDir": "C:\\Users\\pouls\\AppData\\Local\\Google\\Chrome\\User Data\\",
      "trace": true,
      "preLaunchTask": "ng serve"
    },
    {
      "name": "ng test",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    }
  ],
  "compounds": [
    {
      "name": "Debug Solution",
      "presentation": {
        "hidden": false,
        "group": "",
        "order": 1
      },
      "stopAll": true,
      "configurations": [
        "Debug API",
        "Launch Chrome against localhost"
      ],
      "preLaunchTask": "ngrok"
    }
  ]
}